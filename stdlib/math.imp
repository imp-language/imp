
type Math = extern "java.lang.Math"
type Random = extern "java.util.Random"



export func rand() float {
    val rand = new Random()
    return rand.nextFloat()
}

export func rand(start int, end int) int {
    val rand = new Random()
    return rand.nextInt(start, end)
}
