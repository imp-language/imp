// Simple match statement testing on union types

import "batteries"

func testUnion(param int | string | int[]) {
    match param as id {
        int -> {
            log(id)
            log("int")
        }
        string -> {
            log(id)
            log("string")
        }
        int[] -> {
            log(id)
            log("int[]")
        }
    }
}

testUnion(4)
testUnion("s")
testUnion([4,3,2,1])