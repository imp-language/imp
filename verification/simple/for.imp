// tests for loops
import "batteries"

log("range call requiring a separate allocation")
for i in range(5, 15) {
    val b = i
    log(b)
}

log("range with iterator previously allocated")
val iRange = range(0, 10)
for i in iRange {
    val double = i * 2
    log(double)
}

log("for loop sum of range")
mut sum = 0
for i in range(0, 10) {
    sum = sum + i
}
log(sum)