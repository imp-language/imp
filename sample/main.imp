struct Empty {}
struct Leaf {
	data int
}
struct Node {
	left Tree
	right Tree
}
type TreeNode = Tree | Empty
struct Tree {
    data int
    left TreeNode
    right TreeNode
}

func makeTree(root int) Tree {
    val left = Empty()
    val right = Empty()
    val tree = Tree(root, left, right)
    log(left)
    log(right)
    log(tree)
    return tree
}

