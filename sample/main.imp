// tests for loops
import "batteries"



type intOrString = int | string | int[]


func logIntList(a int[]) {
    for i in range(0,len(a)) {
        log(i)
    }
}


func testUnion(param int | int[]) {
    log(intOrString)
    match param as id {
        int -> log(id)
        int[] -> logIntList(id)
    }
}
