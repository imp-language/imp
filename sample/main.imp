// Challenge description: receive positive from stdin and sum. When a -1
// is inputted, stop and output the sum to stdout. Stop and output -1 if
// any inputs are not natural numbers.




// todo: while
mut i = 0
mut sum = 0
while i < 10 {
    val line = readLine()
    sum = sum + int(line)
    i = i + 1
}

log(sum)
